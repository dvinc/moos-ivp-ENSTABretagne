ServerHost = localhost
ServerPort = 9000
Community = ciscreaSaucE

ProcessConfig = ANTLER
{
	MSBetweenLaunches = 100
	Run = MOOSDB					@ NewConsole = false
	Run = pCiscrea					@ NewConsole = true, 	ExtraProcessParams=TemoinTests
	Run = pLocalization				@ NewConsole = true, 	ExtraProcessParams=TemoinTests
	Run = pControler				@ NewConsole = true,	ExtraProcessParams=TemoinTests
	Run = pSonar					@ NewConsole = true, 	ExtraProcessParams=TemoinTests
	Run = pEchoSonder				@ NewConsole = true, 	ExtraProcessParams=TemoinTests
	Run = pModem					@ NewConsole = true, 	ExtraProcessParams=TemoinTests
	Run = pCamera					@ NewConsole = true, 	ExtraProcessParams=TemoinTests
	Run = pHydrophone				@ NewConsole = true, 	ExtraProcessParams=TemoinTests
	Run = pTemperatureCPU			@ NewConsole = true, 	ExtraProcessParams=TemoinTests
	Run = pInertialMeasurementUnit	@ NewConsole = true, 	ExtraProcessParams=TemoinTests
	Run = pLogs						@ NewConsole = true, 	ExtraProcessParams=TemoinTests
	
	TemoinTests = -tests
}